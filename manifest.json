{
  "name": "IwillriL - Read It Later Client",
  "version": "0.1",
  "description": "Unnoficial Read It Later Client Extension for Google Chrome",
  "background_page": "background.html",
  "options_page": "options.html", 
  "browser_action": {
    "default_icon": "bookmark.png",
     "default_popup": "popup.html"
  },
  "permissions": [
    "https://readitlaterlist.com/", 
    "tabs"
  ],
  
  "content_scripts": [
   {
      "matches":  [ "http://*/*", "https://*/*" ],
      "include_globs": [ "http://www.google.*/reader/*", "https://www.google.*/reader/*" ],
      "js": ["myscript.js"]
   }
   ]
}
