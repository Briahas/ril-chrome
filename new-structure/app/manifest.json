{
  "name": "IwillriL - Unnoficial Pocket Client",
  "version": "2.4.1",
  "description": "Unnoficial Pocket Client Extension for Google Chrome",
  "options_page": "views/options.html",
  "browser_action": {
    "default_icon": "images/bookmark.png",
     "default_popup": "index.html"
  },
  "permissions": [
    "https://readitlaterlist.com/",
    "http://readitlaterlist.com/",
    "https://getpocket.com/v3/",
    "tabs",
    "contextMenus",
    "storage"
  ],
   "background": {
    "scripts": ["scripts/chrome-extension-scripts/request.js", "scripts/chrome-extension-scripts/rilList.js", "scripts/chrome-extension-scripts/extensionIcon.js", "scripts/chrome-extension-scripts/background.js"]
  },
   "manifest_version": 2,

   "icons": {
    "16": "images/bookmark.png",
    "48": "images/bookmark48.png",
    "128": "images/bookmark128.png"
  },

  "content_scripts": [
   {
      "matches":  ["<all_urls>"],
      "js": ["scripts/chrome-extension-scripts/myscript.js"]
   }
   ]
}
